{
    "AWSEBDockerrunVersion": "2",
    "containerDefinitions": [
        {
            "name": "app",
            "image": "npcode/mattermost-app:latest",
            "environment": [
                {
                    "name": "DB_PORT_5432_TCP_PORT",
                    "value": "5432"
                }
            ],
            "memory": 128,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ],
            "links": [
                "db"
            ]
        },
        {
            "name": "db",
            "image": "npcode/mattermost-db:latest",
            "memory": 128
        }
    ]
}
